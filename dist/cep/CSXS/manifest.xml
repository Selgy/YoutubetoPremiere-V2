<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest
Version="6.0"
ExtensionBundleId="com.YoutubetoPremiereV2.cep"
ExtensionBundleVersion="$npm_package_new_version"
ExtensionBundleName="YoutubetoPremiere V2"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>
<ExtensionList>
  <Extension Id="com.YoutubetoPremiereV2.cep.main" Version="$npm_package_new_version" />
  <Extension Id="com.YoutubetoPremiereV2.cep.settings" Version="$npm_package_new_version" />
</ExtensionList>
<ExecutionEnvironment>
  <HostList>
    <Host Name="PPRO" Version="[0.0,99.9]" />
  </HostList>
  <LocaleList>
    <Locale Code="All" />
  </LocaleList>
  <RequiredRuntimeList>
    <RequiredRuntime Name="CSXS" Version="9.0" />
  </RequiredRuntimeList>
</ExecutionEnvironment>
<DispatchInfoList>
  <Extension Id="com.YoutubetoPremiereV2.cep.main">
    <DispatchInfo>
      <Resources>
        <MainPath>./main/index.html</MainPath>
        <CEFCommandLine>
          <Parameter>--v=0</Parameter>
          <Parameter>--enable-nodejs</Parameter>
          <Parameter>--mixed-context</Parameter>
          <Parameter>--allow-file-access</Parameter>
        </CEFCommandLine>
      </Resources>
      <Lifecycle>
        <AutoVisible>true</AutoVisible>
      </Lifecycle>
      <UI>
        <Type>Modeless</Type>
        <Menu>YoutubetoPremiere</Menu>
        <Geometry>
          <Size>
            <Width>500</Width>
            <Height>500</Height>
          </Size>
          <MaxSize>
            <Width>1200</Width>
            <Height>800</Height>
          </MaxSize>
          <MinSize>
            <Width>400</Width>
            <Height>350</Height>
          </MinSize>
        </Geometry>
        <Icons>
          <Icon Type="Normal">./src/assets/dark-icon.png</Icon>
          <Icon Type="DarkNormal">./src/assets/light-icon.png</Icon>
          <Icon Type="RollOver">./src/assets/dark-icon.png</Icon>
          <Icon Type="DarkRollOver">./src/assets/light-icon.png</Icon>
        </Icons>
      </UI>
    </DispatchInfo>
  </Extension>
  <Extension Id="com.YoutubetoPremiereV2.cep.settings">
    <DispatchInfo>
      <Resources>
        <MainPath>./settings/index.html</MainPath>
        <CEFCommandLine>
          <Parameter>--v=0</Parameter>
          <Parameter>--enable-nodejs</Parameter>
          <Parameter>--mixed-context</Parameter>
          <Parameter>--allow-file-access</Parameter>
        </CEFCommandLine>
      </Resources>
      <Lifecycle>
        <AutoVisible>false</AutoVisible>
        <StartOn>
          <Event>com.adobe.csxs.events.ApplicationInitialized</Event>
          <Event>applicationActive</Event>
        </StartOn>
      </Lifecycle>
      <UI>
        <Type>Custom</Type>
        
        <Geometry>
          <Size>
            <Width>500</Width>
            <Height>1</Height>
          </Size>
        </Geometry>
        <Icons>
          <Icon Type="Normal">./src/assets/dark-icon.png</Icon>
          <Icon Type="DarkNormal">./src/assets/light-icon.png</Icon>
          <Icon Type="RollOver">./src/assets/dark-icon.png</Icon>
          <Icon Type="DarkRollOver">./src/assets/light-icon.png</Icon>
        </Icons>
      </UI>
    </DispatchInfo>
  </Extension>
</DispatchInfoList>
</ExtensionManifest>