{
  "manifest_version": 3,
  "name": "Youtube to Premiere Pro V2",
  "version": "3.0.1",
  "description": "",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://www.youtube.com/*",
    "https://localhost:3001/*",
    "http://localhost:3001/*",
    "https://127.0.0.1:3001/*",
    "http://127.0.0.1:3001/*"
  ],
  "action": {
    "default_icon": {
      "48": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["socket.io.min.js", "content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
