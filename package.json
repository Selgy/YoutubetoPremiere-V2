{
  "name": "bolt-cep",
  "version": "3.0.0",
  "scripts": {
    "version": "cross-env-shell \"node scripts/version-update.js $npm_package_version && git add -A && (git commit -m v$npm_package_version || true) && git tag -f v$npm_package_version\" && git push && git push --tags -f",
    "postversion": "",
    "prebuild": "node scripts/ensure-dirs.js",
    "clean": "rimraf dist/* node_modules/.vite build/*",
    "build-python": "powershell -ExecutionPolicy Bypass -File build-python.ps1",
    "build-python-mac": "chmod +x ./build-python.sh && ./build-python.sh",
    "build-mac": "yarn clean && tsc -p \"tsconfig-build.json\" && yarn build-python-mac && vite build --force --config vite.config.mac.js",
    "build": "yarn clean && tsc -p \"tsconfig-build.json\" && yarn build-python && vite build --force",
    "build-cross-platform": "node scripts/detect-platform.js",
    "dev": "vite",
    "watch": "yarn clean && tsc -p \"tsconfig.json\" && vite build --force --watch",
    "zxp": "yarn clean && tsc -p \"tsconfig-build.json\" && cross-env SKIP_PYTHON_BUILD=true yarn build-python && cross-env ZXP_PACKAGE=true vite build --force --watch=false",
    "zip": "yarn clean && tsc -p \"tsconfig-build.json\" && yarn build-python && cross-env ZIP_PACKAGE=true vite build --force",
    "serve": "cross-env SERVE_PANEL=true vite preview",
    "symlink": "cross-env ACTION=symlink vite",
    "delsymlink": "cross-env ACTION=delsymlink vite",
    "package": "vite-cep-plugin package"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.4.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^16.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/trusted-types": "^2.0.2",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.17",
    "babel-plugin-react-require": "^3.1.3",
    "babel-plugin-transform-scss": "^1.1.0",
    "babel-preset-env": "^1.7.0",
    "core-js": "3",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.2.0",
    "postcss": "^8.4.35",
    "pyinstaller": "^0.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-node-copy": "^1.0.4",
    "rollup-plugin-scss": "^3.0.0",
    "sass": "^1.43.4",
    "tailwindcss": "^3.4.1",
    "types-for-adobe": "^7.2.0",
    "types-for-adobe-extras": "^0.0.6",
    "typescript": "^4.6.4",
    "vite": "^4.5.2",
    "vite-cep-plugin": "^1.1.12",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-static-copy": "^2.2.0",
    "yarn": "^1.22.17"
  },
  "main": "index.js",
  "license": "MIT",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
